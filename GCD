package programs;
import java.util.Scanner;
public class Gcd {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n1 = sc.nextInt(),n2 = sc.nextInt();
		int min = Math.min(n1, n2), gcd=0;
		for(int i=1; i<=min; i++) {
				if((n1%i==0)&&(n2%i==0)) gcd = i;
		}
		System.out.print(gcd);
		sc.close();
	}
}
